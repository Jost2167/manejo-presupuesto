@model IEnumerable<TipoCuenta>

@{
    ViewData["Title"] = "Tipos Cuentas";
}

<!DOCTYPE html>
<html>
<head>
    <title>title</title>
</head>
<body>
<h1>Lista de tipos de cuentas</h1>
<a asp-action="Crear" class="btn btn-primary mb-3">Crear</a>
<table class="table">
    <thead>
    <tr>
        <th>Nombre</th>
        <th>Acciones</th>
    </tr>
    </thead>
    
    <tbody>
    @foreach (TipoCuenta tipoCuenta in Model)
    {
        <tr>
            <td>
                <a class="btn btn-warning" asp-action="Editar" asp-route-id="@tipoCuenta.Id">Editar</a>
                <a class="btn btn-danger" asp-action="Eliminar" asp-route-id="@tipoCuenta.Id">Eliminar</a>
            </td>
            <td>@tipoCuenta.Nombre</td>
        </tr>
    }
    </tbody>
</table>


<div>

</div>
</body>
</html>